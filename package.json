{
	"name": "dashboard",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@apollo/client": "^3.5.8",
		"@egjs/flicking-plugins": "^4.2.3",
		"@egjs/react-flicking": "^4.4.3",
		"@perifinance/peri-finance": "^0.4.11",
